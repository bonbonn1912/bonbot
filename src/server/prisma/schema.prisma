generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url =""
}

model User {
  userId       Int       @id @default(autoincrement())
  username     String   @unique @db.VarChar(255)
  registeredAt DateTime? @default(now()) @db.Timestamp(6)
  updatedAt    DateTime? @default(now()) @db.Timestamp(6)
  isActive     Boolean
  isAdmin      Boolean
  isBotConnected Boolean
  description  String?   @db.VarChar(500)
  commands Commands[]
}

	model Commands {
  commandId   Int       @id @default(autoincrement())
  trigger     String    @db.VarChar(35)
  value       String    @db.VarChar(500)
  registeredAt DateTime  @default(now())
  updatedAt   DateTime  @updatedAt
  isActive    Boolean
  count       Int
  streamer  String      @db.VarChar(255)
  user        User      @relation(fields: [streamer], references: [username]) // FK auf User
  intervall Int
  isRepetitive Boolean
  isPrivileged Boolean 
}